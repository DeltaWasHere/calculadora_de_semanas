<Window x:Class="calculadora_de_semanas.AverageHistory"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:calculadora_de_semanas"
    mc:Ignorable="d"
    Title="AverageHistory" Height="720" Width="1080">
    <Grid>

        <DataGrid x:Name="entriesToShow" ItemsSource="{Binding}" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Numero" Binding="{Binding place}" />
                <DataGridTextColumn Header="Patron" Binding="{Binding patron}" />
                <DataGridTextColumn Header="Fecha de alta" Binding="{Binding alta}" />
                <DataGridTextColumn Header="Fecha de baja" Binding="{Binding baja}" />
                <DataGridTextColumn Header="Salario" Binding="{Binding salario}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>